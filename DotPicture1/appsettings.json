{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "LiveSassCompileOptions": {
    "EnableLiveCompile": "true",
    "SassFileWatchers": [
      {

        "SourcePath": "wwwroot/Css",
        "DestinationPath": "wwwroot/Css",
        "FileNameFilters": [
          "*.scss"
        ],
        "FileNameExclusions": [
          "_*"
        ],
        "CompileOnStart": true
      }
    ]
  },
  "AllowedHosts": "*"
}
